    <script>
        var calisan = {
            isim: "doruk", soyisim: "ss",
            maas: 1500, departman: "insan kaynakları",
            tamad: function () {
                return this.isim + " " + this.soyisim + " " + this.maas;
            }, zamyap() {
                return this.isim + " kişiye yüzde 5 zam yapıldı" +this.maas*1.05;
            }
        }
        //document.write(calisan.isim);
        //document.write(calisan["soyisim"]);
        var a = calisan.tamad();
        document.write(a);
        var b = calisan.zamyap();
        document.write(b);
    </script>
